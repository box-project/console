{
    "name": "box-project/console",
    "description": "A console application assembled through dependency injection.",
    "keywords": ["console", "dependency", "injection"],
    "license": "MIT",
    "homepage": "http://box-project.org",
    "support": {
        "issues": "https://github.com/box-project/console/issues",
        "source": "https://github.com/box-project/console",
        "wiki": "https://github.com/box-project/console/wiki"
    },

    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io/",
            "role": "Developer"
        }
    ],

    "autoload": {
        "psr-4": {
            "Box\\Component\\Console\\": ""
        }
    },

    "config": {
        "bin-dir": "bin"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },

    "require": {
        "php": ">=5.4",
        "kherge/file": "~1.3",
        "herrera-io/object-storage": "~1.0",
        "symfony/config": "~2.7",
        "symfony/console": "~2.7",
        "symfony/dependency-injection": "~2.7",
        "symfony/yaml": "~2.7"
    },

    "require-dev": {
        "herrera-io/object-storage": "1.*",
        "phpunit/phpunit": "~4.6",
        "symfony/expression-language": "~2.7",
        "symfony/framework-bundle": "~2.7"
    },

    "suggest": {
        "symfony/event-dispatcher": "Enables support for observing console events.",
        "symfony/expression-language": "Allows expressions to be used in service registrations.",
        "symfony/framework-bundle": "Enables commands for debugging service registration."
    }
}
